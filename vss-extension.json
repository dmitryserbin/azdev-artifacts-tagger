{
  "manifestVersion": 1,
  "id": "artifacts-tagger",
  "name": "Artifacts Tagger",
  "version": "1.2.0",
  "publisher": "dmitryserbin",
  "public": true,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Tag Azure DevOps build artifacts from a release or build pipeline",
  "categories": [
    "Azure Pipelines"
  ],
  "tags": [
    "Tag",
    "Release",
    "Build",
    "Pipeline",
    "Deploy"
  ],
  "icons": {
    "default": "Images/icon.png"
  },
  "content": {
    "details": {
      "path": "README.md"
    },
    "changelog": {
      "path": "CHANGELOG.md"
    },
    "privacy": {
      "path": "PRIVACY.md"
    },
    "license": {
      "path": "LICENSE"
    }
  },
  "links": {
    "getstarted": {
      "uri": "https://github.com/dmitryserbin/azdev-artifacts-tagger"
    },
    "support": {
      "uri": "https://github.com/dmitryserbin/azdev-artifacts-tagger/issues"
    },
    "license": {
      "uri": "https://github.com/dmitryserbin/azdev-artifacts-tagger/blob/master/LICENSE"
    },
    "privacypolicy": {
      "uri": "https://github.com/dmitryserbin/azdev-artifacts-tagger/blob/master/PRIVACY.md"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/dmitryserbin/azdev-artifacts-tagger"
  },
  "files": [
    {
      "path": "Tasks"
    },
    {
      "path": "Images",
      "addressable": true
    }
  ],
  "contributions": [
    {
      "id": "artifacts-tagger",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "Tasks"
      }
    }
  ]
}
